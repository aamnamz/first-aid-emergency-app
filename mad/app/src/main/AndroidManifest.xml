<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.madproject">
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MadProject">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoriesActivity" />
        <activity android:name=".WoundActivity" />
        <activity android:name=".CprActivity" />
        <activity android:name=".BurnActivity" />
        <activity android:name=".ChokingActivity" />
        <activity android:name=".FaintingActivity" />
        <activity android:name=".FractureActivity" />
        <activity android:name=".HeatStrokeActivity" />
        <activity android:name=".AsthmaActivity" />
        <activity android:name=".EmergencyContactsActivity" />
        <activity android:name=".MChecklistActivity" />
        <activity android:name=".ToolbarHelper" />
        <activity android:name=".ChecklistsActivity" />
        <activity android:name=".HikingActivity" />
        <activity android:name=".CampingActivity" />
        <activity android:name=".HomefirstaidActivity" />
        <activity android:name=".OutdoorActivity" />
        <activity android:name=".PicnicActivity" />
        <activity android:name=".RoadtripActivity" />
        <activity android:name=".SwimmingActivity" />
        <activity android:name=".TravelActivity" />

    </application>

</manifest>